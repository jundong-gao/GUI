{
  "name": "gui-plus-next",
  "version": "0.1.6",
  "description": "GUI vue3.x前端UI组件库",
  "main": "dist/gui.esm-bundle.js",
  "scripts": {
    "build": "rollup -c",
    "build:theme": "gulp build --gulpfile theme/gulpfile.js && cp-cli theme/lib lib",
    "build:all": "npm run build && npm run build:theme",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jundong-gao/GUI.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "vue3.x",
    "UI",
    "UI组件库"
  ],
  "author": "1643549042@qq.com",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^3.0.0",
    "@vue/compiler-sfc": "^3.2.19",
    "cp-cli": "^2.0.0",
    "glob": "^7.2.0",
    "husky": "4.3.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "rollup-pluginutils": "^2.8.2",
    "standard-version": "^9.3.1",
    "tslib": "^2.3.1",
    "vue": "^3.2.19"
  }
}
